<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>@ViewData["Title"] - Đọc truyện online | Việt Kiếm</title>

    <!-- Css Files -->
    <link href="~/assets/css/bootstrap.css" rel="stylesheet">
    <link href="~/assets/css/font-awesome.css" rel="stylesheet">
    <link href="~/assets/css/flaticon.css" rel="stylesheet">
    <link href="~/assets/css/slick-slider.css" rel="stylesheet">
    <link href="~/assets/css/fancybox.css" rel="stylesheet">
    <link href="~/assets/style.css" rel="stylesheet">
    <link href="~/assets/css/color.css" rel="stylesheet">
    <link href="~/assets/css/responsive.css" rel="stylesheet">


    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <style type="text/css">
        .fancybox-margin {
            margin-right: 17px;
        }

        .ereaders-main-content {
            padding: 0px 0px 60px 0px;
        }

        header#ereaders-header {
            padding: 20px 0px 20px 0px;
        }
        div#search {
            padding: 40px 24px 0px 500px;
        }
        button#search-btn {
            margin: -2px 0px 0px 1px;
        }
        a#read {
            margin: 0px 25px 0px 0px;
        }
        div#main1 {
            margin-top: 50px;
        }
        div#DataTables_Table_0_paginate {
            padding: 0px 200px;
        }
    </style>

    <script type="text/javascript" charset="UTF-8" src="https://maps.googleapis.com/maps-api-v3/api/js/37/10a/common.js"></script>
    <script type="text/javascript" charset="UTF-8" src="https://maps.googleapis.com/maps-api-v3/api/js/37/10a/util.js"></script>

</head>



<body class="js" style="background-color: gainsboro;">
    <!--// Main Wrapper \\-->
    <div class="ereaders-main-wrapper">

        <!--// Header \\-->
        <header id="ereaders-header" class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Hiện menu</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <div class="container">
                <div class="row">
                    <aside class="col-md-3"> <a href=@Url.Action("Index", "Home") class="logo"><img src="~/assets/images/logo.png" alt=""></a> </aside>
                    <aside class="col-md-9">
                        
                        <!--// Navigation \\-->
                        <div class="navbar-collapse collapse" itemscope itemtype="#">
                            <a href="#menu" class="menu-link active"><span></span></a>
                            
                            @using (Html.BeginForm("Search", "Home", FormMethod.Get))
                            {
                                <form method="get" class="navbar-form navbar-right" role="search" itemprop="potentialAction" itemscope="" itemtype="#" style="margin-top: 30px">
                                    <div class="input-group search-holder">
                                        <meta itemprop="target" content="">
                                        <div class="input-group-btn" id="search">
                                            <input class="form-control" id="search-input" type="search" name="search" placeholder="Tìm kiếm..." value="" required="">
                                            <button class="btn btn-default" id="search-btn" type="submit" aria-label="Tìm kiếm" role="search">
                                                <span class="glyphicon glyphicon-search"></span>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="list-group list-search-res hide"></div>
                                </form>
                            }
                        </div>
                        <!--// Navigation \\-->

                    </aside>
                </div>
            </div>
        </header>
        <!--// Header \\-->

        @RenderBody()

        <!--// Footer \\-->
        <footer id="ereaders-footer" class="ereaders-footer-one">

            <!--// Footer Widget \\-->
            <div class="ereaders-footer-widget">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="ereaders-footer-newslatter">
                                <a href=@Url.Action("Index","Home") class="ereaders-footer-logo"><img src="~/assets/images/footer-logo.png" alt=""></a>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <!--// Footer Widget \\-->
            <!--// CopyRight \\-->
            <div class="ereaders-copyright">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <p><i class="fa fa-copyright"></i><a href=@Url.Action("Index","Home")>Việt Kiếm</a> by AH 3.0</p>
                            <ul class="footer-social-network">
                                <li><a href="https://en-gb.facebook.com/login/" class="fa fa-facebook"></a></li>
                                <li><a href="https://www.linkedin.com/uas/login" class="fa fa-linkedin"></a></li>
                                <li><a href="https://twitter.com/login?lang=en" class="fa fa-twitter"></a></li>
                                <li><a href="https://www.pinterest.com/login/" class="fa fa-pinterest-p"></a></li>
                                <li><a href="https://en-gb.facebook.com/login/" class="fa fa-google-plus"></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!--// CopyRight \\-->

        </footer>
        <!--// Footer \\-->

    </div>
    <!--// Main Wrapper \\-->

    <!-- jQuery (necessary for JavaScript plugins) -->
    <script type="text/javascript" src="~/assets/script/jquery.js"></script>
    <script type="text/javascript" src="~/assets/script/jquery-ui.js"></script>
    <script type="text/javascript" src="~/assets/script/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/assets/script/slick.slider.min.js"></script>
    <script type="text/javascript" src="~/assets/script/fancybox.pack.js"></script>
    <script type="text/javascript" src="~/assets/script/isotope.min.js"></script>
    <script type="text/javascript" src="~/assets/script/progressbar.js"></script>
    <script type="text/javascript" src="~/assets/script/jquery.countdown.min.js"></script>
    <script type="text/javascript" src="~/assets/script/circle-chart.js"></script>
    <script type="text/javascript" src="~/assets/script/numscroller.js"></script>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAs_m2ywm-bE8z9YM_WmtPaIQNIosN4Dxo&amp;callback=initMap"></script>
    <script type="text/javascript" src="~/assets/script/functions.js"></script>
